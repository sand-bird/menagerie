config_version=5

[application]

config/name="Menagerie"
run/main_scene="res://system/game.tscn"
config/features=PackedStringArray("4.0")
boot_splash/bg_color=Color(0.721569, 0.945098, 0.972549, 1)
boot_splash/image="res://assets/other/images/sandbird3_transp.png"
boot_splash/fullsize=false
config/icon="res://assets/ui/icons/thumbnail.png"
name="Menagerie"
main_scene="res://system/game.tscn"
icon="res://assets/ui/icons/thumbnail.png"

[autoload]

Log="*res://system/logger.gd"
SaveManager="*res://system/save_manager.gd"
Dispatcher="*res://system/dispatcher.gd"
Data="*res://system/data.gd"
Options="*res://system/options.gd"
PerfectScale="*res://system/perfect_scale.gd"
Player="*res://system/player.gd"
Clock="*res://system/clock.gd"

[debug]

gdscript/warnings/return_value_discarded=false
gdscript/warnings/integer_division=false
gdscript/warnings/unused_argument=false
indicators_enabled=true

[display]

window/size/viewport_width=320
window/size/viewport_height=208
window/size/window_width_override=640
window/size/window_height_override=416
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/stretch/shrink="1"
window/handheld/emulate_touchscreen=true
emulate_touchscreen=true
stretch_mode="viewport"
stretch_aspect="ignore"
test_width=1152
test_height=648
width=384
height=216
use_2d_pixel_snap=true

[editor_plugins]

enabled=PackedStringArray("res://addons/event_button/plugin.cfg")

[gui]

theme/custom_font="res://assets/other/fonts_old/menagerie_white/font.fnt"

[image_loader]

gen_mipmaps=false

[importer_defaults]

texture={
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

