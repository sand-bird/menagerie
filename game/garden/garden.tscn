# class member variables go here, for example:
# var a = 2
# var b = \"textvar\"

func _ready():
	# Called every time the node is added to the scene.
	# Initialization here
	pass

func _pressed():
	var g = get_parent().get_parent()
	for i in range(10):
		var m = load(\"res://monster/monster.tscn\").instance()
		m.position = Vector2(Utils.randi_qty(g.rect_size.x), 
				Utils.randi_qty(g.rect_size.y))
		g.get_node(\"entities\").add_child(m)
		m.get_node(\"sprite\").flip_h = randi() % 2"
