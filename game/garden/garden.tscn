script/source = "extends Control

var points = []

func draw_point(pos: Vector2, radius, color: Color):
	points.append({ pos = pos, radius = radius, color = color })
	queue_redraw()
	
func _process(_delta):
	queue_redraw()

func _draw():
	for p in points:
		draw_circle(p.pos, p.radius, p.color)
"

