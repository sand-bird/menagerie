stretch_mode="viewport"
stretch_aspect="ignore"
test_width=1152
test_height=648
width=384
height=216
use_2d_pixel_snap=true

[editor_plugins]

enabled=PackedStringArray("res://addons/event_button/plugin.cfg", "res://addons/gut/plugin.cfg")

[gui]

theme/custom_font="res://assets/font/menagerieall5.ttf"
